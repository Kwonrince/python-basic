   ],
   "source": [
    "n = int(input())\n",
    "assert n >= 1, \"1 이상의 정수를 입력해 주세요\"\n",
    "\n",
    "for i in range(n):\n",
    "    print(\" \"*(i), \"**\")"
   ]
