   ],
   "source": [
    "natural = int(input())\n",
    "assert natural >= 1, \"자연수를 입력해 주세요\"\n",
    "\n",
    "if natural % 2 == 0:\n",
    "    print(\"even\")\n",
    "else:\n",
    "    print(\"odd\")"
   ]
